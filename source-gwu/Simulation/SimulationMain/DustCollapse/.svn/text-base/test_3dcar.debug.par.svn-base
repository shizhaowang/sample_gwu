#	Runtime parameters for the dust collapse problem.

#	Parameters for initial model

sim_initDens	= 1.E9
sim_initRad	= 6.5E8
sim_tAmbient	= 1.E9
sim_ictr	= 7.E8
sim_jctr	= 7.E8
sim_kctr	= 7.E8

#		Gas ratio of specific heats

gamma           = 1.6666667

#	Computational volume parameters

#		Grid dimensionality and geometry

geometry = "cartesian"

#		Size of computational volume

xmin		= 0.
xmax		= 1.4E9
ymin		= 0.
ymax		= 1.4E9
zmin		= 0.
zmax		= 1.4E9
Nblockx		= 1
Nblocky		= 1
Nblockz		= 1

#		Boundary conditions

xl_boundary_type = "outflow"
xr_boundary_type = "outflow"
yl_boundary_type = "outflow"
yr_boundary_type = "outflow"
zl_boundary_type = "outflow"
zr_boundary_type = "outflow"
grav_boundary_type = "isolated"

#	Simulation (grid, time, I/O) parameters

cfl		= 0.8

lrefine_max     = 4
basenm          = "dustcoll_"
restart         = .false.

checkpointFileIntervalTime          = 0.0358
checkpointFileIntervalStep    =  20

plotFileIntervalTime     = 1000.

nend = 2 #            = 5000

tmax            = 0.0637

refine_var_1	= "dens"
refine_var_2	= "none"

mpole_lmax	= 0
useGravity        = .true.

run_comment     = "Dust collapse problem"
log_file        = "dustcoll.log"

smlrho          = 1.E5
smallp          = 1.E5
smalle          = 1.E1
smallt          = 1.E-7

dtinit           = 1.E-5
dtmax           = 1.E-2
nriem 	         = 50
cvisc           = 0.
eintSwitch     = 1.E-4

memory_stat_freq = 1
convertToConsvdForMeshCalls   = .true.
#monotone        = .true.
