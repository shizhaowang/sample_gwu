!!****if* source/Grid/GridMain/Samrai/Grid_sendOutputData
!!
!! NAME
!!  Grid_sendOutputData
!!
!! SYNOPSIS
!!  #include "Flash.h"
!!
!!  call Grid_sendOutputData(integer(in) :: myPE, 
!!                           integer(in) :: numProcs)
!!  
!! DESCRIPTION 
!!   This routine prepares the Grid_IO data that is needed 
!!   in order to write the data to a checkpoint or plotfile.
!!   Data includes, ngid (nfaces + nchildren + 1 parent), globalOffset
!!   globalNumBlocks and other data that is specific to the grid type, 
!!   Paramesh, UG or other
!!   
!!
!!  ARGUMENTS  
!!    myPE - current processor
!!    numProcs - number of processors
!!
!!***

subroutine Grid_sendOutputData(myPE, numProcs)

implicit none
  integer, intent(in) :: myPE, numProcs

end subroutine Grid_sendOutputData
